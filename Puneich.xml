				<locked type="number">1</locked>
				<name type="string">Messenger - Starting Items</name>
				<source type="string">Messenger</source>
				<text type="formattedtext">
					<h>Parcel</h>
					<linklist>
						<link class="treasureparcel" recordname="treasureparcels.id-00053@Incarnate-Engine">Starting Items - Messenger</link>
