			<id-00001>
				<locked type="number">1</locked>
				<name type="string">Judge - Starting Items</name>
				<source type="string">Judge</source>
				<text type="formattedtext">
					<h>Parcel</h>
					<linklist>
						<link class="treasureparcel" recordname="treasureparcels.id-00046@Incarnate-Engine">Starting Items - Judge</link>
